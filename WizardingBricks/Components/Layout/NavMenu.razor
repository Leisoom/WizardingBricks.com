@inject NavigationManager Navigation

<MudNavMenu>
    <MudNavGroup Title="Sets" Expanded>
        <MudNavLink Href="sets" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">Set List</MudNavLink>
        <MudNavLink Href="edit-set" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Set</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Categories" Expanded>
        <MudNavLink Href="categories" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">Category List</MudNavLink>
        <MudNavLink Href="edit-category" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Category</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Characters" Expanded>
        <MudNavLink Href="characters" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">Character List</MudNavLink>
        <MudNavLink Href="edit-character" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Character</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Minifigures" Expanded>
        <MudNavLink Href="minifigures" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">Minifigure List</MudNavLink>
        <MudNavLink Href="edit-minifigure" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Minifigure</MudNavLink>
    </MudNavGroup>
